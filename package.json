{
  "name": "app",
  "version": "0.4.341",
  "private": true,
  "scripts": {
    "dev": "npm run updateLocales && next-translate && env-cmd -e preprod next dev",
    "dev:prod": "npm run updateLocales && next-translate && env-cmd -e prod next dev",
    "build": "npm run updateLocales && next-translate && env-cmd -e preprod next build",
    "build:prod": "npm run updateLocales && next-translate && env-cmd -e prod next build",
    "start": "next start",
    "lint": "eslint --fix --max-warnings 0 --ext .js,.ts,.tsx .",
    "updateLocales": "ts-node ./scripts/updateLocales",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|json|ts|tsx)\"",
    "check-types": "tsc",
    "format": "npm run prettier -- --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "push:prod": "git pull && git tag -f prod && git push -f --tag",
    "validate": "npm-run-all --parallel check-types lint test build",
    "sls_deploy:preprod": "cp serverless.yml.preprod serverless.yml && cp -R .serverless.preprod .serverless && sls; rm -rf serverless.yml .serverless",
    "sls_deploy:prod": "cp serverless.yml.prod serverless.yml && cp -R .serverless.prod .serverless && sls; rm -rf serverless.yml .serverless"
  },
  "dependencies": {
