    "@types/body-scroll-lock": "^2.6.1",
    "@types/react-color": "^3.0.4",
    "@types/ws": "^7.2.6",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "airtable": "^0.9.0",
    "animate.css": "^4.1.1",
    "apollo-upload-client": "^14.1.1",
    "body-scroll-lock": "^3.1.3",
    "date-fns": "^2.16.1",
    "env-cmd": "^10.1.0",
    "framer-motion": "^2.9.5",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "imagemin-optipng": "^8.0.0",
